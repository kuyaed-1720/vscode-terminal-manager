{
  "name": "vscode-terminal-manager",
  "displayName": "vscode-terminal-manager",
  "description": "A VS Code extension to manage development services",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-terminal-manager.startAllServices",
    "onCommand:vscode-terminal-manager.stopAllServices"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-terminal-manager.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "vscode-terminal-manager.startAllServices",
        "title": "Start All Services"
      },
      {
        "command": "vscode-terminal-manager.stopAllServices",
        "title": "Stop All Services"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
